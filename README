$ ./munj 'lines("test/delim.txt")'
a	b	1	c	d	x
a2	b2	2	c2	d2	x2
	b3	3	c3	d3	x3

$ ./munj 'sum(l.length for (l in lines("test/delim.txt")))'
41

$ ./munj '[l[1] for (l in ila("test/delim.txt"))].join(" ")'
b b2 b3

$ ./munj 'lines("https://raw.github.com/skinner/munj/master/test/delim.txt")'
a	b	1	c	d	x
a2	b2	2	c2	d2	x2
	b3	3	c3	d3	x3

$ ./munj '(l for (l in lines("test/delim.txt")) if (l.match(/b\d/)))'
a2	b2	2	c2	d2	x2
	b3	3	c3	d3	x3

$ ./munj 'range(1, 6)'
1
2
3
4
5

$ ./munj 'take(3, range(1, 6))'
1
2
3

$ ./munj 'drop(2, range(1, 6))'
3
4
5

$ ./munj 'tail(2, range(1, 6))'
4
5

$ ./munj 'sum(range(1, 6))'
15

$ ./munj 'product(range(1, 6))'
120

$ ./munj 'reduce(range(1, 6), function(x, y) {return x + "-" + y}, "")'
-1-2-3-4-5

$ ./munj 'values([4, 8, 15, 16, 23, 42])'
4
8
15
16
23
42

$ ./munj 'sum(values([4, 8, 15, 16, 23, 42]))'
108

